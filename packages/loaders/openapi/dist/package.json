{
  "name": "@omnigraph/openapi",
  "version": "0.19.10",
  "sideEffects": false,
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.3.2",
    "@graphql-mesh/string-interpolation": "0.4.2",
    "@graphql-mesh/types": "0.91.0",
    "@graphql-mesh/utils": "0.43.8",
    "@omnigraph/json-schema": "0.38.9",
    "change-case": "4.1.2",
    "json-machete": "0.18.7",
    "openapi-types": "12.1.0",
    "tslib": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/loaders/openapi"
  },
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "esm/index.js",
